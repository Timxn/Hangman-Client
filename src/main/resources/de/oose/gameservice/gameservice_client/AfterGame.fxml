<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:controller="de.oose.gameservice.gameservice_client.AfterGameController" styleClass="black-bg" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="output_result" styleClass="output-result"/>
    <Label fx:id="output_winner" styleClass="output-winner"/>
    <Label fx:id="output_gameID" styleClass="text-gameid"/>
    <Label fx:id="output_error" styleClass="error-message"/>
    <Button fx:id="button_restart" onAction="#restart" mnemonicParsing="false" text="RESTART" styleClass="button-80"/>
    <Button fx:id="button_quit" onAction="#quit" mnemonicParsing="false" text="QUIT" styleClass="button-80"/>
</AnchorPane>
